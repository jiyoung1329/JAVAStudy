package conditions;

import java.util.Scanner;

public class Quiz07 {
		Scanner sc = new Scanner(System.in);
		
		int day = sc.nextInt();
		int num = day % 7;
		switch(num) {
		case 0 : System.out.println("월요일입니다.");break;
		case 1 : break;
		case 2: break;
		case 3: break;
		case 4: break;
		case 5: break;
		case 6: break;
		}
		
	}

}
