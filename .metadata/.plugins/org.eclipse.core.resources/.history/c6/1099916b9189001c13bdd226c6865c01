package conditions;

import java.util.Scanner;

public class Quiz02 {
	public static void main(String[] args) {
		
		int num1, num2, num3;
		Scanner sc = new Scanner(System.in);
		
		// 세 수를 입력 받아 큰 수 출력
		System.out.println("세 수를 입력하시오. : ");
		num1 = sc.nextInt();
		num2 = sc.nextInt();
		num3 = sc.nextInt();
		
		// 두 수를 입력받아 큰 수가 짝수이면 출력
		System.out.println("두 수를 입력하시오. : ");
		num1 = sc.nextInt();
		num2 = sc.nextInt();
		// 두 수를 입력받아 합이 짝수이고 3의 배수인 수를 출력
		System.out.println("두 수를 입력하시오. : ");
		num1 = sc.nextInt();
		num2 = sc.nextInt();
	}
}
