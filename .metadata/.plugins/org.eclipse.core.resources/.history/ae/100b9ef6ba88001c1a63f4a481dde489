package variables;

public class Quiz1 {
	public static void main(String[] args) {
		String name = "김말이";
		byte age = 20;
		float height = 173.3f;
		byte iq = 120;
		char grade = 'B';
		
		System.out.println("이름 \t :" + name);
		System.out.println("나이 \t :" + age);
		System.out.println("키 \t :" + height);
		System.out.println("아이큐 \t :" + iq);
		System.out.println("등급 \t :" + grade);
	}
}
