package conditions;

import java.util.Scanner;

public class Quiz05 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		// 세 수 중 최대/최소값 구하기
		int num1, num2, num3;
		System.out.println("세 수를 입력하세요. : ");
		num1 = sc.nextInt();
		num2 = sc.nextInt();
		num3 = sc.nextInt();
		
		
		// 정수를 입력받아 3의 배수, 4의 배수 출력
	}
}
