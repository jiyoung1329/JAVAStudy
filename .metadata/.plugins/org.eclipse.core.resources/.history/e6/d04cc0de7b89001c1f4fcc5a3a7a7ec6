package operators;

import java.util.Scanner;

public class Quiz01 {
	public static void main(String[] args) {
		int num, data1, data2;
		
		
		System.out.print("수 입력 : ");
		Scanner sc = new Scanner(System.in);
		num = sc.nextInt();
		System.out.println(num + " = " + (num % 2 == 0)? "짝수": "홀수");
	}
}
