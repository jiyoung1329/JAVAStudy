package loop;

public class Quiz01 {
	public static void main(String[] args) {
		
		int i;
		for (i = 1; i <= 4; i++) {
			System.out.println(i+". hello");
		}
		
		int sum = 0;
		int checkNum = 528;
		for (i=1;i<=100; i++) {
			sum += i;
			if (sum == checkNum) {
				System.out.println("1 ~ " + i + "까지의 합이 "+checkNum+" 입니다.");
			}
		}
	}
}
